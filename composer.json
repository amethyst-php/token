{
    "name": "railken/amethyst-token",
    "type": "library",
    "keywords": ["laravel", "amethyst", "amethyst-token", "token"],
    "license": "MIT",
    "require": {
        "php": ">=7.1",
        "railken/amethyst-common": "0.1.*",
        "ramsey/uuid": "^3.8",
        "railken/amethyst-user": "0.1.*"
    },
    "require-dev": {
        "phpunit/phpunit": "~7.0",
        "orchestra/testbench": "3.6.*",
        "railken/amethyst-authentication": "0.1.*"
    },
    "autoload": {
        "psr-4" : {
            "Railken\\Amethyst\\" : "src/"
        }
    },
    "autoload-dev": {
        "psr-4": { 
            "Railken\\Amethyst\\Tests\\": "tests/" 
        }
    },
    "extra": {
        "amethyst": {
            "package": "token",
            "namespace": "Railken\\Amethyst"
        },
        "laravel": {
            "providers": [
                "Railken\\Amethyst\\Providers\\TokenServiceProvider"
            ]
        }
    }
}
